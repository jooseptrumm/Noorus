{
  "name": "noorus",
  "version": "1.0.0",
  "description": "website for mixed choir Noorus",
  "main": "server.js",
  "scripts": {
    "watch:sass": "node-sass public/sass/main.scss public/css/style.css -w",
    "compile:sass": "node-sass public/sass/main.scss public/css/style.comp.css",
    "concat:css": "concat -o public/css/style.conc.css public/css/style.comp.css public/css/all.css",
    "prefix:css": "postcss --use autoprefixer -o public/css/style.pref.css public/css/style.conc.css",
    "compress:css": "node-sass public/css/style.pref.css public/css/style.css --output-style compressed"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jooseptrumm/Noorus.git"
  },
  "author": "Joosep Trumm",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jooseptrumm/Noorus/issues"
  },
  "homepage": "https://github.com/jooseptrumm/Noorus#readme",
  "dependencies": {
    "ejs": "^2.7.1",
    "express": "^4.17.1",
    "multer": "^1.4.2",
    "mysql": "^2.17.1",
    "node-sass": "^4.13.1",
    "nodemailer": "^6.4.16",
    "nodemon": "^2.0.6",
    "path": "^0.12.7"
  }
}
